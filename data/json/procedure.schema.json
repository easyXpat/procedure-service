{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Procedure",
  "required" : [
    "id",
    "name",
    "description",
    "city"
  ],
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "description": "UUID to identify the procedure"
    },
    "name": {
      "type": "string",
      "description": "name of the procedure"
    },
    "description": {
      "type": "string",
      "description": "description"
    },
    "city": {
      "type": "string",
      "description": "City assigned to this procedure"
    },
    "created_at": {
      "type": "string",
      "description": "UTC datetime when record was created",
      "format": "date-time"
    },
    "updated_at": {
      "type": "string",
      "description": "UTC datetime when record was last updated",
      "format": "date-time"
    }
  }
}